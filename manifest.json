{
    "manifest_version": 2,
    "name": "Copy Load Info",
    "description": "Easily copy the lane info",
    "version": "1.0",
    "icons": {
        "16": "/images/icon_16.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "/images/icon_16.png",
        "default_popup": "popup.html",
        "default_title": "Copy The Load Info"
    }
}